echo "# star on list"
echo '["a", "b", "c"]' | ../../lib/jsontool.js -x '*'

echo "# star on list of objects"
echo '[{"one": "un"}, {"two": "deux"}]' | ../../lib/jsontool.js -x '*'

echo "# star on list of objects, sub lookup"
echo '[{"foo": "bar"}, {"foo": "baz"}]' | ../../lib/jsontool.js -x '*.foo'
